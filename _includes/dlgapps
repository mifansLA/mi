<script>
function getQueryVariable(e){for(var r=window.location.search.substring(1),t=r.split("&"),n=0;n<t.length;n++){var a=t[n].split("=");if(a[0]==e)return a[1]}return!1}window.onload=function(){var klik=f=getQueryVariable("arch"),s=getQueryVariable("variant"),e=getQueryVariable("api"),x="https://opengapps.org/?download=true&";document.getElementById("download").innerHTML=f+" "+s+" "+e,document.getElementById("download").href=x+"api="+e+"&arch="+f+"variant="+s};</script>
