<script>function getQueryVariable(e){for(var r=window.location.search.substring(1),t=r.split("&"),n=0;n<t.length;n++){var a=t[n].split("=");if(a[0]==e)return a[1]}return!1}window.onload=function(){var klik=z=getQueryVariable("api"),y=getQueryVariable("arch"),e=getQueryVariable("variant"),x="https://opengapps.org/?download=true&";document.getElementById("download").innerHTML=z+" "+y+" "+e",document.getElementById("download").href=x+"&api="+z+"&arch="+y+"&variant="+e};</script>
