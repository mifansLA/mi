<script>function getQueryVariable(e){for(var r=window.location.search.substring(1),t=r.split("&"),n=0;n<t.length;n++){var a=t[n].split("=");if(a[0]==e)return a[1]}return!1}window.onload=function(){var klik=v=getQueryVariable("ver"),t=getQueryVariable("type"),e=getQueryVariable("name");document.getElementById("download").innerHTML=t+"_"+v+"_"+e,document.getElementById("download").href="http://bigota.d.miui.com/"+v+"/"+t+"_"+v+"_"+e};</script>
