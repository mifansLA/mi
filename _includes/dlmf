<script>
function getQueryVariable(e){for(var r=window.location.search.substring(1),t=r.split("&"),n=0;n<t.length;n++){var a=t[n].split("=");if(a[0]==e)return a[1]}return!1}window.onload=function(){var klik=f=getQueryVariable("name"),s=getQueryVariable("size"),e=getQueryVariable("file"),x="http://mediafire.com/file/";document.getElementById("download").innerHTML=f+" "+s,document.getElementById("download").href=x+e+"/"+f+"/file";document.getElementById("notice").innerHTML="ready to download"}</script>
