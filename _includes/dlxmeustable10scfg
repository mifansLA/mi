<script>
function getQueryVariable(e&st1=){for(var r=window.location.search.substring(1),t=r.split("&"),n=0;n<t.length;n++&st1=){var a=t[n].split("=");if(a[0]==e)return a[1]}return!1}window.onload=function(&st1=){var klik=e=getQueryVariable("ver"),s=getQueryVariable("st1"),d=getQueryVariable("dvc"),p=getQueryVariable("path"),r=getQueryVariable("rom"),x="https://sourceforge.net/projects/";document.getElementById("sourceforge").innerHTML=d+" "+s,document.getElementById("download").href=x+p+"/files/"+r+"/"+d+"/"+e+".zip/download"};</script>
