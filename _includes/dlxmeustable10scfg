<div style="display:block;text-align:center"><a href="/" id="scfg" class="btn btn--primary">Fetching link from Sourceforge. Wait...</a></div>
<script>function getQueryVariable(e){for(var r=window.location.search.substring(1),t=r.split("&"),n=0;n<t.length;n++){var a=t[n].split("=");if(a[0]==e)return a[1]}return!1}window.onload=function(){var klik=e=getQueryVariable("ver"),s=getQueryVariable("st1"),d=getQueryVariable("dvc"),p=getQueryVariable("path"),r=getQueryVariable("rom"),x="https://sourceforge.net/projects/";document.getElementById("scfg").innerHTML=d+" "+e+" "+s,document.getElementById("scfg").href=x+p+"/files/"+r+"_"+d+"_"+e+".zip/download"};</script>
