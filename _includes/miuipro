<script>function getQueryVariable(e){for(var r=window.location.search.substring(1),t=r.split("&"),n=0;n<t.length;n++){var a=t[n].split("=");if(a[0]==e)return a[1]}return!1}window.onload=function(){var klik=v=getQueryVariable("device"),e=getQueryVariable("name");document.getElementById("miuipro").innerHTML=e+" "+e,document.getElementById("miuipro").href="https://ota.miuipro.by/roms/"+v+"/"+e};</script>
